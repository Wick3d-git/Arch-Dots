 {
      "pyprland": {
        "plugins": ["scratchpads", "magnify"]
      },
      "scratchpads": {
        "term": {
          "command": "kitty --class scratchpad",
          "margin": 50,
          "animation": "fromTop"
        },
        "ranger": {
          "command": "kitty --class scratchpad -e ranger",
          "margin": 50,
          "animation": "fromTop"
        },
        "volume": {
          "command": "kitty --class scratchpad -e pulsemixer",
          "margin": 50,
          "animation": "fromTop"
        }
      }
    }
